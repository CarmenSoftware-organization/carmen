{
  "priceLifecycleStates": [
    {
      "id": "draft",
      "name": "Draft",
      "description": "Price is being prepared but not yet active",
      "color": "#6B7280",
      "isActive": false,
      "allowTransitions": ["pending_approval", "cancelled"]
    },
    {
      "id": "pending_approval",
      "name": "Pending Approval",
      "description": "Price is awaiting approval before becoming active",
      "color": "#F59E0B",
      "isActive": false,
      "allowTransitions": ["active", "rejected", "cancelled"]
    },
    {
      "id": "active",
      "name": "Active",
      "description": "Price is currently valid and in use",
      "color": "#10B981",
      "isActive": true,
      "allowTransitions": ["expiring", "suspended", "cancelled"]
    },
    {
      "id": "expiring",
      "name": "Expiring Soon",
      "description": "Price will expire within the warning period",
      "color": "#F97316",
      "isActive": true,
      "allowTransitions": ["expired", "renewed", "extended"]
    },
    {
      "id": "expired",
      "name": "Expired",
      "description": "Price validity period has ended",
      "color": "#EF4444",
      "isActive": false,
      "allowTransitions": ["renewed", "archived"]
    },
    {
      "id": "suspended",
      "name": "Suspended",
      "description": "Price is temporarily inactive",
      "color": "#8B5CF6",
      "isActive": false,
      "allowTransitions": ["active", "cancelled"]
    },
    {
      "id": "renewed",
      "name": "Renewed",
      "description": "Price has been renewed with new validity period",
      "color": "#06B6D4",
      "isActive": true,
      "allowTransitions": ["active", "expiring"]
    },
    {
      "id": "cancelled",
      "name": "Cancelled",
      "description": "Price has been cancelled and is no longer valid",
      "color": "#64748B",
      "isActive": false,
      "allowTransitions": ["archived"]
    },
    {
      "id": "archived",
      "name": "Archived",
      "description": "Price is archived for historical reference only",
      "color": "#9CA3AF",
      "isActive": false,
      "allowTransitions": []
    }
  ],
